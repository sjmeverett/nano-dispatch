{
  "name": "nano-dispatch",
  "description": "A universal, lightweight dispatcher",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": [
      {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },

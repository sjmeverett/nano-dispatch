      },
      "./dist/matchers/equalMatcher.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "node --test --import=@swc-node/register/esm-register"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@swc-node/register": "^1.10.10",
    "@types/node": "22.7.9",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.26.1"
  }
